<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sayuri.panaderiahu.controllers.AgregarGalletaController">
   <children>
      <Pane prefHeight="400.0" prefWidth="120.0" style="-fx-background-color: #F3D6AD;" />
      <Label layoutX="246.0" layoutY="28.0" text="Agregar galleta" textAlignment="RIGHT">
         <font>
            <Font name="Century751 No2 BT Bold" size="27.0" />
         </font>
      </Label>
      <VBox layoutX="141.0" layoutY="100.0" prefHeight="200.0" prefWidth="240.0">
         <children>
            <Label text="Nombre de la galleta">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textGalleta" />
            <Label text="Costo">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textCosto" />
            <Label text="Tipo de galleta">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textTipo" />
         </children>
      </VBox>
      <Button fx:id="bttonAgregar" layoutX="246.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#bttonAgregar" text="Agregar" />
      <Button fx:id="bttonVolver" layoutX="396.0" layoutY="325.0" mnemonicParsing="false" onMouseClicked="#bttonVolver" text="Volver" />
   </children>
</AnchorPane>
