<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sayuri.panaderiahu.controllers.AgregarPastelController">
   <children>
      <Pane prefHeight="400.0" prefWidth="125.0" style="-fx-background-color: #F3D6AD;" />
      <Label layoutX="248.0" layoutY="29.0" text="Agregar pastel">
         <font>
            <Font name="Century751 No2 BT Bold" size="27.0" />
         </font>
      </Label>
      <VBox layoutX="148.0" layoutY="93.0" prefHeight="200.0" prefWidth="226.0" spacing="5.0">
         <children>
            <Label text="Nombre del pastel">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textPastel" />
            <Label text="Costo">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textCosto" />
            <Label text="Sabor">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textSabor" />
         </children>
      </VBox>
      <Button fx:id="bttonAgregar" layoutX="263.0" layoutY="330.0" mnemonicParsing="false" onMouseClicked="#bttonAgregar" text="Agregar" />
      <Button fx:id="bttonVolver" layoutX="461.0" layoutY="330.0" mnemonicParsing="false" onMouseClicked="#bttonVolver" text="Volver" />
   </children>
</AnchorPane>
