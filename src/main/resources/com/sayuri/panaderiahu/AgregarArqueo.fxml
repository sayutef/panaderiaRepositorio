<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="486.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.sayuri.panaderiahu.controllers.AgregarArqueoController">
    <children>
        <Pane prefHeight="492.0" prefWidth="53.0" style="-fx-background-color: #F3D6AD;" />
        <Label layoutX="192.0" layoutY="14.0" text="Agregar arqueo" textAlignment="RIGHT">
            <font>
                <Font name="Century751 No2 BT Bold" size="27.0" />
            </font>
        </Label>
        <VBox layoutX="66.0" layoutY="66.0" prefHeight="319.0" prefWidth="353.0">
            <children>
                <Label text="Ingrese cantidad de panes vendidos">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Label>
                <TextField fx:id="textCantPanes" />
                <Label text="Ingrese cantidad de pasteles vendidos">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Label>
                <TextField fx:id="textCantPastel" />
                <Label text="Ingrese cantidad de galletas vendidas">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Label>
                <TextField fx:id="textCantGalleta" />
            <Label text="Ingrese costo del pan vendido">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textCostoPanVendido" />
            <Label text="Ingrese costo del pastel vendido">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <TextField fx:id="textCostoPastelVendido" />
            <Label text="Ingrese costo de las galletas vendidas">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <TextField fx:id="textCostoGalletaVendida" />
            </children>
        </VBox>
        <Button fx:id="bttonAgregar" layoutX="192.0" layoutY="443.0" mnemonicParsing="false" onMouseClicked="#bttonAgregar" text="Agregar" />
        <Button fx:id="bttonVolver" layoutX="329.0" layoutY="443.0" mnemonicParsing="false" onMouseClicked="#bttonVolver" text="Volver" />
    </children>
</AnchorPane>
